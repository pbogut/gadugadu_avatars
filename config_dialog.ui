  <!-- interface-naming-policy toplevel-contextual -->
  <object class="GtkWindow" id="window1">
    <child>
      <object class="GtkVBox" id="gadugadu_avatars_config_vbox">
        <property name="visible">True</property>
        <property name="border_width">9</property>
        <property name="orientation">vertical</property>
        <property name="spacing">4</property>
        <child>
          <object class="GtkLabel" id="gadugadu_transport_domain_label">
            <property name="visible">True</property>
            <property name="can_focus">False</property>
            <property name="label" translatable="yes">Transport domain</property>
          </object>
        </child>
        <child>
          <object class="GtkEntry" id="gadugadu_transport_domain">
            <property name="tooltip_text" translatable="yes">Domain of the gg transport server (ie. gg.uaznia.net).</property>
            <property name="visible">True</property>
            <property name="can_focus">True</property>
            <property name="invisible_char">‚óè</property>
          </object>            
        </child>
      </object>
    </child>
  </object>
</interface>